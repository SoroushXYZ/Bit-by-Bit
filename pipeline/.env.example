# Stock Data API Keys
# Copy this file to pipeline/.env and add your actual API keys

# Finnhub API (Primary)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Alpha Vantage API (Fallback)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Polygon.io API (Fallback)
POLYGON_API_KEY=your_polygon_api_key_here

TOGETHER_AI_API_KEY=your_together_api_key_here

# Environment Configuration (development or production)
ENVIRONMENT=development

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1
AWS_PROFILE= # Leave empty if using access keys

# S3 Buckets
AWS_S3_BUCKET_DEV=bit-by-bit-dev-data
AWS_S3_BUCKET_PROD=bit-by-bit-prod-data

# Upload Configuration
UPLOAD_TO_AWS=false